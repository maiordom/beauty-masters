[ignore]
.*/node_modules/react-native/Libraries/.*/*.js
.*/node_modules/react-native/local-cli/.*/*.js
.*/node_modules/react-native/packager/.*/*.js
.*/node_modules/react-native/lib/.*/*.js
.*/node_modules/react-native-experimental-navigation/.*

[include]

[libs]
node_modules/react-native/flow
flow/

[lints]

[options]
module.system=haste
module.system.node.resolve_dirname=apps

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
unsafe.enable_getters_and_setters=true

munge_underscores=false
